  "private": true,
  "dependencies": {
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-router-dom": "^4.3.0"
  },
  "scripts": {
    "start:dev": "better-npm-run start:dev",
    "build": "better-npm-run compile"
  },
  "betterScripts": {
    "compile": {
      "command": "node scripts/build",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start:dev": {
      "command": "node scripts/start:dev",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*",
        "PORT": 3001
      }
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
